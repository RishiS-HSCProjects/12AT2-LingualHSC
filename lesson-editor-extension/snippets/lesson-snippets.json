{
  "Info Note": {
    "prefix": "/i",
    "body": ["/i ${1:note text} \\"],
    "description": "Insert an info note"
  },
  "Warning Note": {
    "prefix": "/w",
    "body": ["/w ${1:warning text} \\"],
    "description": "Insert a warning note"
  },
  "Tip Note": {
    "prefix": "/t",
    "body": ["/t ${1:tip text} \\"],
    "description": "Insert a tip note"
  },
  "Color Tag": {
    "prefix": "::color",
    "body": ["::${1:blue}{${2:text}}"],
    "description": "Insert a color tag"
  },
  "Blockquote Block": {
    "prefix": ":::blockquote",
    "body": [":::blockquote ${1:content} :::"],
    "description": "Insert a blockquote block"
  },
  "Spoiler": {
    "prefix": "spoiler",
    "body": ["||${1:text}||"],
    "description": "Insert spoiler text"
  },
  "Quiz Embed": {
    "prefix": "quiz",
    "body": ["~quizzes:${1:lesson}:${2:id}~"],
    "description": "Insert a quiz embed"
  },
  "Lesson Frontmatter": {
    "prefix": "lesson-frontmatter",
    "body": [
      "---",
      "title: \"${1:Title}\"",
      "slug: \"${2:slug}\"",
      "tags: [\"${3:grammar}\", \"${4:particles}\"]",
      "summary: \"${5:Summary}\"",
      "---",
      "",
      "$0"
    ],
    "description": "Insert lesson frontmatter"
  }
}
